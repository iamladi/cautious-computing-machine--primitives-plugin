{
  "name": "primitives-plugin",
  "version": "1.8.0",
  "description": "Essential building blocks for Claude Code workflows",
  "private": true,
  "type": "module",
  "scripts": {
    "validate": "bun run scripts/validate-plugin.ts",
    "validate:versions": "bun run scripts/validate-versions.ts",
    "release:patch": "bun run scripts/release.ts patch",
    "release:minor": "bun run scripts/release.ts minor",
    "release:major": "bun run scripts/release.ts major",
    "eval": "bun run eval/run-eval.ts",
    "eval:baseline": "bun run eval/run-eval.ts --mode structural"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@types/bun": "latest",
    "zod": "^3.23.8"
  }
}
